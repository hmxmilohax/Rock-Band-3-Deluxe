         (score_box {foreach $entry {file_list "ui/track/score_box/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (stars {foreach $entry {file_list "ui/track/stars/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (font {foreach $entry {file_list "ui/track/font/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (smasher {foreach $entry {file_list "ui/track/smashers/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (flames {foreach $entry {file_list "ui/track/flames/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (rails {foreach $entry {file_list "ui/track/rails/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (lanes {foreach $entry {file_list "ui/track/lanes/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
