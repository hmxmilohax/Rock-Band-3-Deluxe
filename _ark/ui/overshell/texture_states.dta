         (spotlight {foreach $entry {file_list "ui/track/spotlights/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (multiplier_ring {foreach $entry {file_list "ui/track/multiplier_ring/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (crowd_meter {foreach $entry {file_list "ui/track/crowd_meter/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (gems {foreach $entry {file_list "ui/track/gems/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
         (keyboard_lanes {foreach $entry {file_list "ui/track/keyboards/gen/*.png_ps3"} {if {!= $entry ""} {push_back $options $entry}}})
