         (animated_gems {set $options ("rb4")})
      }
      {if {== $dx_tracked_texture_loader highway}
         ($user_highway_array
            {array 0})
         {set $user_highway_array {file_list "GD:/dev_hdd0/game/BLUS30463/USRDIR/custom_textures/highways/gen/*.png_ps3"}}
         {if {> {size $user_highway_array} 0}
            {foreach $entry $user_highway_array {if {!= $entry ""} {push_back $options $entry}}}
