{func
   dx_game_var_reset
   {dx_log_writer info {sprint "func: dx_game_var_reset - Current Screen: " {ui current_screen} " - Gamemode: " $dx_gamemode}}
   {dx_log_writer info
      {sprintf "Executed dx_game_var_reset - time: %.4fms"
         {time
            {set $guitar_milosong FALSE} {set $bass_milosong FALSE} {set $drum_milosong FALSE} {set $keys_milosong FALSE}
            {set $real_guitar_milosong FALSE} {set $real_drum_milosong FALSE} {set $real_bass_milosong FALSE} {set $real_keys_milosong FALSE}
            {set $guitar_caughtmissed FALSE} {set $bass_caughtmissed FALSE} {set $drum_caughtmissed FALSE} {set $keys_caughtmissed FALSE}
            {set $real_guitar_caughtmissed FALSE} {set $real_bass_caughtmissed FALSE} {set $real_drum_caughtmissed FALSE} {set $real_keys_caughtmissed FALSE}
            {set $guitar_firstnote FALSE} {set $bass_firstnote FALSE} {set $drum_firstnote FALSE} {set $keys_firstnote FALSE}
            {set $real_guitar_firstnote FALSE} {set $real_bass_firstnote FALSE} {set $real_drum_firstnote FALSE} {set $real_keys_firstnote FALSE}
            {set $guitar_missed_once FALSE} {set $bass_missed_once FALSE} {set $drum_missed_once FALSE} {set $keys_missed_once FALSE}
            {set $real_guitar_missed_once FALSE} {set $real_bass_missed_once FALSE} {set $real_drum_missed_once FALSE} {set $real_keys_missed_once FALSE}
            {set $num_gems_combo_guitar FALSE} {set $num_gems_combo_bass FALSE} {set $num_gems_combo_drum FALSE} {set $num_gems_combo_keys FALSE}
            {set $num_gems_combo_real_guitar FALSE} {set $num_gems_combo_real_bass FALSE} {set $num_gems_combo_real_drum FALSE} {set $num_gems_combo_real_keys FALSE}
            {set $guitar_groove_active FALSE} {set $bass_groove_active FALSE} {set $drum_groove_active FALSE}
            {set $dx_someone_fcd FALSE} {set $dx_someone_niced FALSE} {set $dx_someone_jurgend FALSE}
         }
      }
   }
}
