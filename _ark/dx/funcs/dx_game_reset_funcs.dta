{func
   dx_overdrive_color_var_reset
   {dx_log_writer info {sprint "func: dx_overdrive_color_var_reset - Current Screen: " {ui current_screen} " - Gamemode: " $dx_gamemode}}
   {dx_log_writer info
      {sprintf "Executed dx_overdrive_color_var_reset - time: %.4fms"
         {time
