#define DX_MTV_SETUP_TEXT
(
   {{{gamemode get track_panel} find mtv_overlay} setup_text} ;refresh the mtv overlay
)
#define DX_MTV_BPM_HANDLER
(
   {if {&& {modifier_mgr is_modifier_active mod_dx_showbpm} $dx_mtvup}
      {set $dx_tmp {int {+ 0.5 {seconds_to_beat {/ {beatmatch get_song_ms} 1000}}}}}
      {set $dx_bpm_counter {* {* 60 {/ 1 {- {beat_to_seconds {+ $dx_tmp 1}} {beat_to_seconds $dx_tmp}}}} $speedmod}}
      DX_MTV_SETUP_TEXT
   }
)
#define DX_MTV_SET_POS_HANDLER
(
   (dx_set_mtv_pos
      {if_else {modifier_mgr is_modifier_active mod_fakejuke} ;check if fake jukebox mode is enabled
         {Group.grp set_local_pos 0 -30 -136} ;if it is, hard set the location of the mtv overlay for style
         {Group.grp set_local_pos $mtvpos_0 $mtvpos_1 $mtvpos_2} ;otherwise put it in the custom location
      }
   )
)
#define DX_RANK_VAR_HANDLER
(
   {foreach $entry $rankvar
      {if {== {elem $entry 0} {basename rank}}
         {if {elem {find $entry guitar} 1}
            {set $guitarrankvar {elem {find $entry guitar} 1}}
         }
      }
      {if_else {>= $guitarrankvar 409}
         {set $guitarrankvar 6}
         {if_else {&& {>= $guitarrankvar 333} {< $guitarrankvar 409}}
            {set $guitarrankvar 5}
            {if_else {&& {>= $guitarrankvar 267} {< $guitarrankvar 333}}
               {set $guitarrankvar 4}
               {if_else {&& {>= $guitarrankvar 221} {< $guitarrankvar 267}}
                  {set $guitarrankvar 3}
                  {if_else {&& {>= $guitarrankvar 176} {< $guitarrankvar 221}}
                     {set $guitarrankvar 2}
                     {if_else {&& {>= $guitarrankvar 139} {< $guitarrankvar 176}}
                        {set $guitarrankvar 1}
                        {if {< $guitarrankvar 139}
                           {set $guitarrankvar 0}
                        }
                     }
                  }
               }
            }
         }
      }
   }
   {foreach $entry $rankvar
      {if {== {elem $entry 0} {basename rank}}
         {if {elem {find $entry drum} 1}
            {set $drumrankvar {elem {find $entry drum} 1}}
         }
      }
      {if_else {>= $drumrankvar 448}
         {set $drumrankvar 6}
         {if_else {&& {>= $drumrankvar 345} {< $drumrankvar 448}}
            {set $drumrankvar 5}
            {if_else {&& {>= $drumrankvar 242} {< $drumrankvar 345}}
               {set $drumrankvar 4}
               {if_else {&& {>= $drumrankvar 178} {< $drumrankvar 242}}
                  {set $drumrankvar 3}
                  {if_else {&& {>= $drumrankvar 151} {< $drumrankvar 178}}
                     {set $drumrankvar 2}
                     {if_else {&& {>= $drumrankvar 124} {< $drumrankvar 151}}
                        {set $drumrankvar 1}
                        {if {< $drumrankvar 124}
                           {set $drumrankvar 0}
                        }
                     }
                  }
               }
            }
         }
      }
   }
   {foreach $entry $rankvar
      {if {== {elem $entry 0} {basename rank}}
         {if {elem {find $entry bass} 1}
            {set $bassrankvar {elem {find $entry bass} 1}}
         }
      }
      {if_else {>= $bassrankvar 436}
         {set $bassrankvar 6}
         {if_else {&& {>= $bassrankvar 364} {< $bassrankvar 436}}
            {set $bassrankvar 5}
            {if_else {&& {>= $bassrankvar 293} {< $bassrankvar 364}}
               {set $bassrankvar 4}
