      {set $play_a_show_found_tempo_part FALSE}
      {set $play_a_show_found_fem_part FALSE}
      {set $play_a_show_found_ugc_part FALSE}

      {set $num {random_int 0 {{song_select_panel find song.lst} num_data}}}
