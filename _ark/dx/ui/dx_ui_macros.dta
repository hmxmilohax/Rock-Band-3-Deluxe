      (3 "sv3_panel")
      (4 "sv4_panel")
      (5 "sv5_panel")
      (6 "sv5_panel")
      (7 "sv7_panel")
      (8 "sv8_panel")
      (9 "sv8_panel")
      (10 "sv8_panel")
      kDataUnhandled
   }
)
#define DX_SV_PANEL
(
   #ifdef HX_WII
   SV_PANEL_HANDLER
   #else
   {if_else {file_exists MENU_BACKGROUND_BIK_PATH}
      song_movie_panel
